{
  "name": "napifit",
  "version": "0.1.35",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "echo 'Skipping prisma generate in postinstall (will run in build)'",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:deploy": "prisma migrate deploy",
    "d1:create": "wrangler d1 create napifit-db",
    "d1:migrate": "wrangler d1 migrations apply napifit-db",
    "d1:migrate:remote": "wrangler d1 migrations apply napifit-db --remote",
    "d1:local": "wrangler d1 execute napifit-db --local --file=./prisma/schema.sql",
    "d1:remote": "wrangler d1 execute napifit-db --remote --file=./prisma/schema.sql",
    "d1:backup": "wrangler d1 export napifit-db --output=./backup.sql",
    "db:setup": "npm run prisma:generate && npm run prisma:migrate && npm run d1:create",
    "version:update": "node scripts/version-update.js",
    "git:commit": "node scripts/git-commit-push.js",
    "git:setup": "node scripts/git-setup.js",
    "deploy": "node scripts/auto-deploy.js",
    "deploy:prepare": "node scripts/prepare-deploy.js",
    "cap:sync": "npx cap sync",
    "cap:copy": "npx cap copy",
    "android:open": "npx cap open android",
    "ios:open": "npx cap open ios",
    "android:build": "npm run build && npm run cap:sync",
    "ios:build": "npm run build && npm run cap:sync",
    "cloudflare:build": "npm run build && opennextjs-cloudflare build && node scripts/prepare-cloudflare-output.js",
    "cloudflare:preview": "npm run cloudflare:build && opennextjs-cloudflare preview"
  },
  "dependencies": {
    "@capacitor/app": "^7.1.0",
    "@capacitor/browser": "^7.0.2",
    "@capacitor/camera": "^7.0.2",
    "@capacitor/preferences": "^7.0.2",
    "@next-auth/prisma-adapter": "1.0.7",
    "@opennextjs/cloudflare": "^1.12.0",
    "@prisma/client": "^6.19.0",
    "bcryptjs": "2.4.3",
    "date-fns": "^3.6.0",
    "next": "14.2.5",
    "next-auth": "4.24.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/ios": "^7.4.4",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "20.12.12",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "8.4.38",
    "prisma": "^6.19.0",
    "sharp": "^0.34.5",
    "tailwindcss": "3.4.10",
    "typescript": "5.5.4",
    "wrangler": "^4.38.0"
  }
}
